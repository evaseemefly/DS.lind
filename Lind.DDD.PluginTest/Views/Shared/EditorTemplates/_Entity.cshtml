@model object
@Html.EditorForModel()
<p>实体</p>
@foreach (var item in Model.GetType().GetProperties().Where(i => i.PropertyType.IsClass
    && i.PropertyType != typeof(string)))
{ 
    <p>@item.Name</p>
    
}
<p>集合</p>
@foreach (var item in Model.GetType().GetProperties().Where(i => typeof(System.Collections.IEnumerable).IsAssignableFrom(i.PropertyType)
    && i.PropertyType != typeof(string)))
{ 
    <p>@item.Name</p>
}

