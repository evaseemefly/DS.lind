@{
    ViewBag.Title = "详细";
}
@model WebLogger
<script src="~/Scripts/jquery-2.0.0.js"></script>
@Html.BindToDetailMore(Model, @<div>
    <div class="form-group">
        <label class="col-sm-2">请求参数</label>
        <div class="col-sm-10" style="word-break: break-all;">
            <table id="requestParam" class="table table-hover table-bordered table-striped"></table>
        </div>
    </div>
</div>, i=>i.UserId
,i=>i.UserName
,i => i.ControllerName
, i => i.ActionName
, i => i.ActionName
, i => i.Description)
<script type="text/javascript">
    var json = JSON.parse('@Html.Raw(Model.RequestParams)');
    if ($.isEmptyObject(json)) {
        $("#requestParam").append("<tr><td>无参数</td></tr>");
    } else {
        for (var i in json) {
            $("#requestParam").append("<tr><td><em>" + i + "</em></td><td>" + json[i] + "</td></tr>");
        }
    }


</script>

